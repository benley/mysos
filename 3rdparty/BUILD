MESOS_VERSION = '0.27.0'

python_requirement_library(
  name = 'mesos.interface',
  requirements = [
    python_requirement(
      requirement = 'mesos.interface==%s' % MESOS_VERSION,
    )
  ]
)

python_requirement_library(
  name = 'mesos.native',
  requirements = [
    python_requirement(
      requirement = 'mesos.native==%s' % MESOS_VERSION,
    )
  ]
)

python_requirements()
